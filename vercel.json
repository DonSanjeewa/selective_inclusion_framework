{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex",
          "has": [
            {
              "type": "host",
              "value": "(.*)\\.vercel\\.app"
            }
          ]
        }
      ]
    }
  ]
}
