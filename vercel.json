{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "selective-inclusion-framework.vercel.app"
        }
      ],
      "destination": "https://www.selectiveinclusionframework.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "selective-inclusion-framework.vercel.app"
        }
      ],
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ]
}
